{
  "data_sources": [
    {
      "name": "narrpr",
      "type": "api",
      "enabled": true,
      "priority": 1,
      "polling_interval": 3600,
      "batch_size": 100,
      "endpoint": "https://api.narrpr.com/v1/properties",
      "auth_type": "api_key",
      "params": {
        "limit": 100,
        "format": "json"
      },
      "headers": {
        "User-Agent": "BCBSValues/1.0",
        "Content-Type": "application/json"
      },
      "timeout": 60,
      "retry_count": 3,
      "retry_delay": 5,
      "features": [
        "property_basic_info",
        "valuation_history",
        "tax_assessment"
      ]
    },
    {
      "name": "mls",
      "type": "api",
      "enabled": true,
      "priority": 2,
      "polling_interval": 7200,
      "batch_size": 50,
      "endpoint": "https://api.mlsservice.com/v2/listings",
      "auth_type": "api_key",
      "params": {
        "limit": 50,
        "status": "active,pending,sold",
        "days_back": 30
      },
      "headers": {
        "User-Agent": "BCBSValues/1.0",
        "Content-Type": "application/json"
      },
      "timeout": 30,
      "retry_count": 3,
      "retry_delay": 10,
      "features": [
        "listing_details",
        "sales_price",
        "days_on_market"
      ]
    },
    {
      "name": "pacs",
      "type": "api",
      "enabled": true,
      "priority": 3,
      "polling_interval": 21600,
      "batch_size": 200,
      "endpoint": "https://api.pacs.gov/v1/assessments",
      "auth_type": "api_key",
      "params": {
        "limit": 200,
        "assessment_year": "current"
      },
      "headers": {
        "User-Agent": "BCBSValues/1.0",
        "Content-Type": "application/json"
      },
      "timeout": 120,
      "retry_count": 5,
      "retry_delay": 15,
      "features": [
        "tax_assessment",
        "property_details",
        "improvement_details"
      ]
    }
  ],
  "extraction": {
    "property_id_field": "id",
    "batch_processing": true,
    "parallel_execution": true,
    "threads": 4,
    "timeout": 7200,
    "error_threshold": 0.05
  },
  "transformation": {
    "schema_validation": true,
    "field_mappings": "configs/field_mappings.json",
    "data_cleaning": true,
    "outlier_detection": true,
    "outlier_method": "iqr",
    "outlier_threshold": 1.5,
    "missing_value_strategy": "impute_mean"
  },
  "loading": {
    "batch_size": 100,
    "commit_strategy": "batch",
    "upsert": true,
    "id_field": "property_id",
    "parallel_loading": true,
    "threads": 2,
    "error_handling": "log_and_continue"
  },
  "validation": {
    "validate_output": true,
    "log_level": "INFO",
    "notification_channel": "api",
    "notification_url": "/api/etl-status",
    "validation_rules": [
      {
        "field": "property_value",
        "rule": "range",
        "min": 1000,
        "max": 100000000
      },
      {
        "field": "square_feet",
        "rule": "range",
        "min": 100,
        "max": 50000
      },
      {
        "field": "year_built",
        "rule": "range",
        "min": 1800,
        "max": 2025
      }
    ]
  },
  "scheduling": {
    "cron_expression": "0 0 * * *",
    "timezone": "America/New_York",
    "max_concurrent_jobs": 2,
    "retry_failed": true,
    "max_retries": 3,
    "backoff_factor": 1.5
  }
}