{
  "commandName": "WebApp",
  "runCommands": ["python3 start_webapp.py"],
  "displayName": "Web Application",
  "restartOn": {
    "files": ["start_webapp.py", "templates/**", "static/**"]
  },
  "env": {
    "FLASK_APP": "start_webapp.py",
    "FLASK_ENV": "development",
    "FLASK_DEBUG": "1"
  },
  "port": 5002
}
